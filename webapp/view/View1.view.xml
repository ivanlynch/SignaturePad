<mvc:View controllerName="pad2.PadTest2.controller.View1" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
	xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<core:HTML id="html"></core:HTML>
						<Button id="Signature" text="Signature" press="onSign"></Button>
						<Button id="Save" text="Save" press="saveButton"></Button>
						<Button id="clear" text="Clear" press="clearButton"></Button>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>